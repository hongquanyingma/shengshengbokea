    <!--[if lt IE 9]>
        <script src="/admin/bootstrap/js/html5shiv.min.js"></script>
        <script src="/admin/bootstrap/js/respond.min.js"></script>
    <![endif]-->
	<style>
	    .aa tr td{
		       padding:10px;
		 }
		 .cc{
		 padding:10px;
		 padding-left:10px;
		 padding-right:55px;
		 background-color:#f2f2f2
		 }
	     .bb{
		 padding:10px;
		 padding-left:10px;
		 padding-right:55px;
		 }
	
	</style>
	
	
	
  </head>
  <body class="skin-blue sidebar-mini">
    <div class="wrapper">
      
         <header class="main-header">
        <!-- Logo -->
        <a href="index2.html" class="logo">
          <!-- 对于侧边栏迷你50x50像素迷你标志 -->
          <span class="logo-mini"><%=adminuser%></span>
          <!-- 正常状态和移动设备标识 -->
          <span class="logo-lg">博客后台管理</span>
        </a>
        <!-- Header Navbar: style can be found in header.less -->
        <nav class="navbar navbar-static-top" role="navigation">
          <!-- Sidebar toggle button-->
          <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
            <span class="sr-only">切换导航</span>
          </a>
          <div class="navbar-custom-menu">
            <ul class="nav navbar-nav">
           
            </ul>
          </div>
        </nav>
     </header>
	 
      
      <!-- Left side column. contains the logo and sidebar -->
      <aside class="main-sidebar">
        <!-- sidebar: style can be found in sidebar.less -->
        <section class="sidebar">
          <!-- Sidebar user panel -->
          <div class="user-panel">
            <div class="pull-left image">
              <img src="/dist/img/user2-160x160.jpg" class="img-circle" alt="User Image" />
            </div>
            <div class="pull-left info">
              <p><%=adminuser%></p>

              <a href="#"><i class="fa fa-circle text-success"></i> 在线</a>
            </div>
          </div>
          <!-- sidebar menu: : style can be found in sidebar.less -->
          <ul class="sidebar-menu">
            <li class="header">主导航</li>
            <li class="active treeview">
              <a href="#">
                <i class="fa fa-dashboard"></i> <span>博客信息管理</span> <i class="fa fa-angle-left pull-right"></i>
              </a>
              <ul class="treeview-menu">
                <li class="active"><a href="index.html"><i class="fa fa-circle-o"></i> 浏览博客信息</a></li>
                <li><a href="index2.html"><i class="fa fa-circle-o"></i> 发布博客信息</a></li>
              </ul>
            </li>
            <li class="treeview">
              <a href="#">
                <i class="fa fa-files-o"></i>
                <span>博客分类管理</span>
                <span class="label label-primary pull-right">4</span>
              </a>
              <ul class="treeview-menu">
                <li><a href="pages/layout/top-nav.html"><i class="fa fa-circle-o"></i> 浏览分类信息</a></li>
                <li><a href="pages/layout/boxed.html"><i class="fa fa-circle-o"></i> 添加分类信息</a></li>
              </ul>
            </li>
           
            <li class="treeview">
              <a href="#">
                <i class="fa fa-pie-chart"></i>
                <span>多栏列表项</span>
                <i class="fa fa-angle-left pull-right"></i>
              </a>
              <ul class="treeview-menu">
                <li><a href="/users"><i class="fa fa-circle-o"></i> 会员列表</a></li>
                <li><a href="/tags"><i class="fa fa-circle-o"></i> 标签列表</a></li>
                <li><a href="/titles"><i class="fa fa-circle-o"></i> 文章列表</a></li>
                <li><a href="/comms"><i class="fa fa-circle-o"></i> 评论列表</a></li>
              </ul>
            </li>
            
          
          </ul>
        </section>
        <!-- /.sidebar -->
      </aside>
	  
	  
	  
	  
	  

      <!-- Content Wrapper. Contains page content -->
      <div class="content-wrapper">
        <!-- Content Header (Page header) -->
        <section class="content-header">
          <h1>
            当前文章信息
            <small>修改</small>
          </h1>
          <ol class="breadcrumb">
            <li><a href="/admin"><i class="fa fa-dashboard"></i> 更改页</a></li>
            <li><a href="#">文章信息</a></li>
          </ol>
        </section>

        <!-- Main content -->
        <section class="content">
          <div class="row">
            <div class="col-md-12">
              <!-- Horizontal Form -->
              <div class="box box-primary">
                <div class="box-header with-border">
                  <h3 class="box-title"><i class="fa fa-plus"></i>更新文章信息</h3>
                </div><!-- /.box-header -->
                <!-- form start -->
                <form action="/titles/doUpdate?id=<%=vo.a_id%>" method="GET">
            <table width="300" border="0"  class="aa"  >
 
                <tr>
                    <td >文章ID：</td>
                    <td><input type="text" readonly="true"  class="cc" name="a_id"  onfocus=this.blur() value="<%=vo.a_id%>"/></td>
                </tr>
                <tr>
                    <td>用户ID：</td>
                    <td><input type="text" name="user_id"  class="bb" value="<%=vo.user_id%>"/></td>
                </tr>
			    <tr>
                    <td>标签ID：</td>
                    <td><input type="text" name="tag_id"  class="bb"  value="<%=vo.tag_id%>"/></td>
                </tr>
			    <tr>
                    <td>标题：</td>
                    <td><input type="text" name="title"  class="bb"  value="<%=vo.title%>"/></td>
                </tr>
				 <tr>
                    <td>作者：</td>
                    <td><input type="text" name="author"  class="bb"  value="<%=vo.author%>"/></td>
                </tr>
				 <tr>
                    <td>地址：</td>
                    <td><input type="text" name="address"  class="bb"  value="<%=vo.address%>"/></td>
                </tr>
				 <tr>
                    <td>概要：</td>
                    <td><input type="text" name="abstracts"  class="bb"  value="<%=vo.abstracts%>"/></td>
                </tr>
				 <tr>
                    <td>详文：</td>
                    <td><input type="text" name="message"  class="bb"  value="<%=vo.message%>"/></td>
                </tr>
				 <tr>
                    <td>时间：</td>
                    <td><input type="text" name="time"  class="bb"  value="<%=vo.time%>"/></td>
                </tr>
				 <tr>
                    <td>点击量：</td>
                    <td><input type="text" name="click_num"  class="bb"  value="<%=vo.click_num%>"/></td>
                </tr> 
				<tr>
                    <td>是否推荐：</td>
                    <td><input type="text" name="booleanrec"  class="bb"  value="<%=vo.booleanrec%>"/></td>
                </tr>
                
                <tr>
                    <td colspan="5" align="right">
                       <input type="reset" class="btn btn-default"  data-dismiss="modal" value="重置"/>
                       <input type="submit" class="btn btn-primary" value="提交"/>
                    </td>
                </tr>
            </table>
            </form>
				<div class="row"><div class="col-sm-12">&nbsp;</div></div>
              </div><!-- /.box -->
              
            </div><!-- /.col -->
            
          </div><!-- /.row -->
         
        </section><!-- /.content -->
        
      </div><!-- /.content-wrapper -->
      
      <footer class="main-footer">
        <div class="pull-right hidden-xs">
          <b>Version</b> 7.2
        </div>
        <strong>Copyright &copy; 2016-2017 <a href="http://almsaeedstudio.com">圣圣博客-博客开发</a>.</strong> All rights reserved.
      </footer>
      
      
      
      <!-- Add the sidebar's background. This div must be placed
           immediately after the control sidebar -->
      <div class='control-sidebar-bg'></div>
    </div><!-- ./wrapper -->
    
    <!-- jQuery 2.1.4 -->
    <script src="/plugins/jQuery/jQuery-2.1.4.min.js"></script>
    <!-- Bootstrap 3.3.2 JS -->
    <script src="/bootstrap/js/bootstrap.min.js" type="text/javascript"></script>    
    
    <!-- Slimscroll -->
    <script src="/plugins/slimScroll/jquery.slimscroll.min.js" type="text/javascript"></script>
    <!-- FastClick -->
    <script src='/plugins/fastclick/fastclick.min.js'></script>
    <!-- AdminLTE App -->
    <script src="/dist/js/app.min.js" type="text/javascript"></script>    

    <!-- AdminLTE 用于演示目的 -->
    <script src="/dist/js/demo.js" type="text/javascript"></script>

  </body>
</html>
